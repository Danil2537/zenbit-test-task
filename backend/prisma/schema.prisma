generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum Providers {
    local
    jwt
    google
    github
}

model Building {
    id Int @id @default(autoincrement())
    title String
    price Int
    tiket Int 
    yield Float 
    sold Float
    daysLeft Int
    imageURL String

}

model User {
    id Int @id @default(autoincrement())
    // username String
    email String 
    password String?
    provider Providers @default(local)
}